{
  "name": "HJ Adblock",
  "manifest_version": 3,
  "version": "0.1",
  "action": {
    "default_title": "HJ Adblock",
    "default_popup": "popup.html"
  },
  "content_security_policy": {
  "extension_pages": "default-src 'none'",
  "sandbox": "default-src 'none'"
  }
  "declarative_net_request" : {
    "rule_resources" : [{
      "id": "gamemonetize",
      "enabled": true,
      "path": "gm_rules.json"
    },
                       {
      "id": "gamedistribution",
      "enabled": true,
      "path": "gd_rules.json"
    }]
  },
  "description": "HJ Adblock",
  "incognito": "spanning",
  "homepage_url": "https://htmljatekok.xyz/?ref=hjadblock",
  "offline_enabled": "true",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "short_name": "HJ Adblock"
}
