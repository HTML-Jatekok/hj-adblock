{
  "name": "HJ Adblock",
  "manifest_version": 3,
  "version": "0.1",
  "action": {
    "default_title": "HJ Adblock",
    "default_popup": "popup.html"
  },
  "content_security_policy": {
  "extension_pages": "default-src 'none'; style-src 'unsafe-inline'"
  },
  "declarative_net_request" : {
    "rule_resources" : [{
      "id": "gamemonetize",
      "enabled": true,
      "path": "gm_rules.json"
    },
                       {
      "id": "gamedistribution",
      "enabled": true,
      "path": "gd_rules.json"
    },
                       {
      "id": "gamemonetize_unsafe",
      "enabled": true,
      "path": "gm_rules_unsafe.json"
    }]
  },
  "description": "HJ Adblock",
  "icons": {
    "32": "32.png",
    "48": "48.png",
    "64": "64.png",
    "128": "128.png",
    "180": "180.png",
    "192": "192.png",
    "256": "256.png",
    "512": "512.png"
  },
  "incognito": "spanning",
  "homepage_url": "https://htmljatekok.xyz/?ref=hjadblock",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "short_name": "HJ Adblock"
}
